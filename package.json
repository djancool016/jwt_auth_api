{
  "name": "auth-api",
  "version": "1.0.0",
  "description": "Personal project to implement clean architecture for authentication and authorization service",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "migration-test": "cross-env NODE_ENV=test npx knex migrate:latest --knexfile ./src/knexfile.ts",
    "seed-test": "cross-env NODE_ENV=test npx knex seed:run --knexfile ./src/knexfile.ts",
    "setup-test": "npm run migration-test && npm run seed-test",
    "start-test": "cross-env NODE_ENV=test node dist/index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand",
    "migration-dev": "cross-env NODE_ENV=development npx knex migrate:latest --knexfile ./src/knexfile.ts",
    "seed-dev": "cross-env NODE_ENV=development npx knex seed:run --knexfile ./src/knexfile.ts",
    "setup-dev": "npm run migration-dev && npm run seed-dev",
    "start-dev": "cross-env NODE_ENV=development node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "db-errors": "^0.2.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inversify": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  }
}
